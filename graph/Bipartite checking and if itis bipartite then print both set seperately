
//question link:https://codeforces.com/problemset/problem/687/A


#include<iostream>
#include<vector>
#define ll long long int
using namespace std;
vector<vector<ll>> v1;
ll n1;
void checkBipartite(ll n,ll color[],ll visited[])
{
    visited[n]=1;
    for(int i=0;i<v1[n].size();i++)
    {
        if(visited[v1[n].at(i)]==0)
        {
            color[v1[n].at(i)]=1-color[n];
            checkBipartite(v1[n].at(i),color,visited);
        }
        else
        if(color[v1[n].at(i)]==color[n])
        color[n1]=4;
    }
}

int main()
{
    ll n,m;
    cin>>n>>m;
    v1.resize(n+5);
    ll u,v;
    n1=n+1;
    for(int i=0;i<m;i++)
    {
        cin>>u>>v;
        v1[u].push_back(v);
        v1[v].push_back(u);
    }
    ll color[n+2];
    ll visited[n+2];
    for(int i=0;i<=n+1;i++)
    {
        color[i]=-1;
        visited[i]=0;
    }

    for(int i=1;i<=n;i++)
    {
        if(visited[i]==0)
        {
           color[i]=1;
           checkBipartite(i,color,visited);
        }
    }
    if(color[n1]==4)
    cout<<-1<<endl;
    else
    {
        int zero=0,one=0;
        for(int i=1;i<=n;i++)
        {
             if(color[i]==0)
             zero++;
             if(color[i]==1)
             one++;
        }
        cout<<zero<<endl;
        for(int i=1;i<=n;i++)
        if(color[i]==0)
        cout<<i<<" ";
        cout<<endl;
        cout<<one<<endl;
        for(int i=1;i<=n;i++)
        if(color[i]==1)
        cout<<i<<" ";
        cout<<endl;       
    }
    return(0);
}
